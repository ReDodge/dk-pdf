<?xml version="1.0"?>
<ruleset name="DKPDF Coding Standard">

    <file>./src</file>
    <file>./dk-pdf.php</file>

    <arg value="sp"/>

    <config name="text_domain" value="dkpdf"/>

    <rule ref="Inpsyde"/>
